{
  "name": "elec-renamer",
  "version": "1.0.0",
  "description": "Electron app that rebuilds the JRenamer",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "jest --config ./jest.node.config.js",
    "test-coverage": "jest --config ./jest.node.config.js --coverage",
    "lint": "eslint --config ./.eslintrc.json . --fix",
    "pack": "build --dir",
    "dist": "build"
  },
  "keywords": [
    "electron",
    "renamer"
  ],
  "homepage": "https://www.laal.dev",
  "engines": {
    "node": ">=12.0.0"
  },
  "author": {
    "name": "Lars Albrecht",
    "email": "mail@laal.dev",
    "url": "https://www.laal.dev"
  },
  "license": "MIT",
  "devDependencies": {
    "electron-builder": "^20.40.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.5.1",
    "jest": "^24.8.0"
  },
  "dependencies": {
    "electron": "^5.0.1",
    "commander": "^2.20.0",
    "date-fns": "^1.30.1",
    "drag-drop": "^4.2.0"
  },
  "build": {
    "appId": "laal.elec.renamer",
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Utility"
    }
  }
}
