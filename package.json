{
  "name": "elec-renamer",
  "version": "1.0.0",
  "description": "Electron app that rebuilds the JSRenamer",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "jest --config ./jest.node.config.js",
    "test-browser": "jest --config jest.browser.config.js",
    "test-coverage": "jest --config ./jest.node.config.js --coverage",
    "lint": "eslint --config ./.eslintrc.json . --fix",
    "pack": "build --dir",
    "dist": "build"
  },
  "keywords": [
    "electron",
    "renamer",
    "javascript",
    "html",
    "css"
  ],
  "homepage": "https://www.laal.dev",
  "engines": {
    "node": ">=12.0.0"
  },
  "author": {
    "name": "Lars Albrecht",
    "email": "mail@laal.dev",
    "url": "https://www.laal.dev"
  },
  "repository": {
    "url": "https://github.com/larsalbrecht/elec-renamer"
  },
  "license": "MIT",
  "devDependencies": {
    "electron-builder": "^20.44.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^22.21.0",
    "jest": "^24.9.0"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "commander": "^2.20.3",
    "date-fns": "^1.30.1",
    "drag-drop": "^4.2.0",
    "electron": "^9.4.0"
  },
  "build": {
    "appId": "laal.elec.renamer",
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Utility"
    },
    "win": {
      "target": [
        "portable",
        "nsis",
        "msi",
        "zip",
        "tar.gz"
      ]
    }
  }
}
